<h1>Listing channels</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Provider</th>
      <th>Client</th>
      <th>Client secret</th>
      <th>Last login at</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @channels.each do |channel| %>
      <tr>
        <td><%= channel.name %></td>
        <td><%= channel.provider %></td>
        <td><%= channel.client_id %></td>
        <td><%= channel.client_secret %></td>
        <td><%= channel.last_login_at %></td>
        <td><%= channel.user %></td>
        <td><%= link_to 'Show', channel %></td>
        <td><%= link_to 'Edit', edit_channel_path(channel) %></td>
        <td><%= link_to 'Destroy', channel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Channel', new_channel_path %>
